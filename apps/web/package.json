    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
