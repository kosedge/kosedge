    "@prisma/client-runtime-utils": "7.4.0",
    "bcryptjs": "^2.4.3",
    "gray-matter": "^4.0.3",
    "next": "16.1.3",
    "next-auth": "^5.0.0-beta.25",
    "next-mdx-remote": "^6.0.0",
    "pg": "^8.13.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
